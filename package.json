{
  "name": "e-commerce-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --ssl=true --ssl-key=./ssl-localhost/ssl-localhost.key --ssl-cert=./ssl-localhost/ssl-localhost.crt --configuration=development --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production --aot=true",
    "watch": "ng build --watch --configuration development",
    "test": "jest --verbose",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "dev:ssr": "ng run e-commerce-app:serve-ssr --open",
    "serve:ssr": "node dist/e-commerce-app/server/main.js",
    "build:ssr": "ng build && ng run e-commerce-app:server",
    "prerender": "ng run e-commerce-app:prerender",
    "lint": "ng lint && stylelint \"src/**/*.{css,scss}\"",
    "prepare": "husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^16.2.10",
